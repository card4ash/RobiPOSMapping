<style>
    .trpos {
        cursor: pointer;
    }

        .trpos:hover {
            background-color: lightyellow;
        }
     .panel-info > .panel-heading {
        color: #3a87ad;
        background-color: #ffcb05;
        border-color: #ffcb05;
        color: #0B0A0A;
    }
    #layer {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: #000;
        opacity: .8;
        z-index: 100003;
        display: none;
        overflow: hidden;
    }

    #popup {
        width: 90%;
        height: 95%;
        position: fixed;
        margin: auto;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        z-index: 100013;
        border: 1px solid green;
        padding: 10px;
        background-color: #fff;
        display: none;
    }

    #pickEmployee {
        height: 100%;
        width: 100%;
        overflow-y: scroll;
        overflow-x:hidden;
    }

    #mapcanvas {
        height: 90%;
        width: 100%;
    }

    .boxclose {
        float: right;
        margin-top: -30px;
        margin-right: -30px;
        cursor: pointer;
        color: #fff;
        border: 1px solid #AEAEAE;
        border-radius: 30px;
        background: #605F61;
        font-size: 31px;
        font-weight: bold;
        display: inline-block;
        line-height: 0px;
        padding: 11px 3px;
    }
</style>

<script>
    $(function () {
        $('#boxclose').click(function () {
            $('#btnUpdate').hide();
            $('#btnCancel').hide();
            $('#btnVerify').show();
            $('#btnEdit').show();

            $('#txtRetailerName').hide();
            $('#txtAddress').hide();
            $('#cmbRetailerType').hide();
            $('#txtElMsisdn').hide();
            $('#cmbVisitDay').hide();
            $('#cmbStructure').hide();
            $('#cmbShopSignage').hide();
            $('#cmbShopType').hide();
            $('#cmbLocality').hide();

            $('.tdRetailerName').show();
            $('.tdAddress').show();
            $('.tdRetailerType').show();
            $('.tdElMsisdn').show();
            $('.tdVisitDay').show();
            $('.tdrStructure').show();
            $('.tdShopSignage').show();
            $('.tdShopType').show();
            $('.tdLocality').show();
            $('#popup,#layer').hide();
        });
        $('#layer').click(function () {
            $('#btnUpdate').hide();
            $('#btnCancel').hide();
            $('#btnVerify').show();
            $('#btnEdit').show();

            $('#txtRetailerName').hide();
            $('#txtAddress').hide();
            $('#cmbRetailerType').hide();
            $('#txtElMsisdn').hide();
            $('#cmbVisitDay').hide();
            $('#cmbStructure').hide();
            $('#cmbShopSignage').hide();
            $('#cmbShopType').hide();
            $('#cmbLocality').hide();

            $('.tdRetailerName').show();
            $('.tdAddress').show();
            $('.tdRetailerType').show();
            $('.tdElMsisdn').show();
            $('.tdVisitDay').show();
            $('.tdrStructure').show();
            $('.tdShopSignage').show();
            $('.tdShopType').show();
            $('.tdLocality').show();
            $('#popup,#layer').hide();
        });
    });

</script>
