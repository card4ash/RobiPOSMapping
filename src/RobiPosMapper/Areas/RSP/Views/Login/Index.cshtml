                                    </label>
                                </section>

                                <section>
                                    <label class="label">Password</label>
                                    <label class="input">
                                        <i class="icon-append fa fa-lock"></i>
                                        <input type="password" name="password" value="0">
                                        <b class="tooltip tooltip-top-right"><i class="fa fa-lock txt-color-teal"></i> Enter your password</b>
                                    </label>
                                    
                                </section>

                            </fieldset>
                            <footer>
                                <button type="submit" class="btn btn-warning" style="background-color:#ffcb05;border-color:#ffcbff">
                                    Sign in
                                </button>
                            </footer>
                        </form>

                    </div>

                  

