    <script type="text/javascript">
        runAllForms();

        $(function () {
            // Validation
            $("#login-form").validate({


                //-->submit handler
                submitHandler: function (form) {
                    var postdata = $("#login-form").serialize();

                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("Index","Login")',
                        async: true,
                        data: postdata,

                        beforeSend: function () {
                            //$('#loginBtn').attr('disabled', 'disabled');
                            //$('#loginBtn').html('checking');
                        },
                        success: function (response) {

                            if (response.result == 'InvalidLoginName') {
                                alert("Login name does not exist.");
                                //$('#invalidlogin').removeClass('hidden-alert');
                                //$('#loginBtn').removeAttr("disabled");
                                //$('#loginBtn').html('Try Again');
                            }
                            else if (response.result == 'InvalidPassword') {
                                alert("Invalid Password.");

                                //alert('Error.');  //$('#testHtml').html('Database Error.'); return false;
                                //$('#loginBtn').removeAttr("disabled");
                                //$('#loginBtn').html('Try Again');
                            }
                            else if (response.result == 'Error') {
                                alert('Error.');  //$('#testHtml').html('Database Error.'); return false;
                                //$('#loginBtn').removeAttr("disabled");
                                //$('#loginBtn').html('Try Again');
                            }
                            else if (response.result == 'Redirect') {
                                //$('#loginBtn').removeAttr("disabled");
                                //$('#loginBtn').html('Login');
                                window.location = response.url;
                            }
                        },
                        error: function (request, status, error) {

                            alert("Failed to execute. Error Details -  Request: " + request + ", Status: " + status + ", Error: " + error);
                            //$('#loginBtn').removeAttr("disabled");
                            //$('#loginBtn').html('Try Again');
                        },
                        complete: function () {

                        }
                    });
                },

                //<--submit handler




                // Rules for form validation
                rules: {
                    loginname: {
                        required: true
                    },
                    password: {
                        required: true
                    }
                },

                // Messages for form validation
                messages: {
                    loginname: {
                        required: 'Please enter your email address'
                    },
                    password: {
                        required: 'Please enter your password'
                    }
                },

                // Do not change code below
                errorPlacement: function (error, element) {
                    error.insertAfter(element.parent());
                }
            });
        });
    </script>

