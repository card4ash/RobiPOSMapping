
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <title>@ViewBag.Title</title>

    <!-- FAVICONS -->
    <link rel="shortcut icon" href="~/Assets/MyImages/dashboardfavicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Assets/MyImages/dashboardfavicon.ico" type="image/x-icon">

    <link href="~/Assets/Amsterdam/css/main.css" rel="stylesheet" type="text/css" />
    <!--[if IE]> <link href="css/ie.css" rel="stylesheet" type="text/css"> <![endif]-->

    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/jquery_ui_custom.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/charts/excanvas.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/charts/jquery.flot.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/charts/jquery.flot.resize.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/charts/jquery.sparkline.min.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.tagsinput.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.inputlimiter.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.autosize.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.ibutton.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.dualListBox.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.validate.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.uniform.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.select2.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/forms/jquery.cleditor.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/uploader/plupload.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/uploader/plupload.html4.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/uploader/plupload.html5.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/uploader/jquery.plupload.queue.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/wizard/jquery.form.wizard.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/wizard/jquery.form.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.collapsible.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.timepicker.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.jgrowl.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.pie.chart.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.fullcalendar.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.elfinder.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/ui/jquery.fancybox.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/tables/jquery.dataTables.min.js"></script>

    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/bootstrap/bootstrap-bootbox.min.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/bootstrap/bootstrap-progressbar.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/plugins/bootstrap/bootstrap-colorpicker.js"></script>
    <script src="~/Assets/Plugins/pnotify/jquery.pnotify.min.js"></script>
    <script src="~/Assets/js/notifications.js"></script>
    <script type="text/javascript" src="~/Assets/Amsterdam/js/functions/custom.js"></script>
   

</head>

<body>

    <!-- Top nav -->
    <div id="top">
        <div class="top-wrapper">
            <a href="index" title=""><img src="~/Assets/Amsterdam/images/logo.png" class="logo" alt="" /></a>
            <ul class="topnav">
                <li class="topuser">
                    <a title="" data-toggle="dropdown"><img src="~/Assets/Amsterdam/images/user.png" alt="" /><span>@ViewBag.UserName</span></a>
                </li>
                <li class="sidebar-button"><a href="#" title=""><b class="responsive-nav"></b></a></li>
                <li><a href="/robiAdmin/Login" title=""><b class="logout"></b></a></li>
            </ul>
        </div>
    </div>
    <!-- /top nav -->

    <!-- Main wrapper -->
    <div class="wrapper three-columns">

        <!-- Left sidebar -->
        <div class="sidebar" id="left-sidebar">
            <ul class="navigation standard">
                <!-- standard nav -->
                <li class="@SetClass("Dashboard")">
                    <a href="~/RobiAdmin/Home/Index?user=@ViewBag.UserName&moid=@ViewBag.MoId" title=""><img src="~/Assets/Amsterdam/images/icons/mainnav/dashboard.png" alt="" />Dashboard</a>
                </li>
                <li class="@SetClass("Surveyor Management")">
                    <a href="~/RobiAdmin/SurveyorManagement/Index?user=@ViewBag.UserName&moid=@ViewBag.MoId" title="">Surveyor Management</a>
                </li>


                <li class="@SetClass("Overlapping Retailers")">
                    <a href="~/RobiAdmin/OutletList/OverlappingOutletList?user=@ViewBag.UserName&moid=@ViewBag.MoId" title="Retailers where multipe surveyor worked.">Overlapping Retailers</a>
                </li>

                <li>
                    <a href="~/RobiAdmin/Reports/SrRetailerStats?user=@ViewBag.UserName&moid=@ViewBag.MoId" title="Surveyor wise updated and new pos count till today.">Surveyors Work Summary</a>
                </li>

                <li class="@SetClass("Search")">
                    <a href="~/RobiAdmin/OutletSearch/Index?user=@ViewBag.UserName&moid=@ViewBag.MoId" title="Search with various criteria.">Search</a>
                </li>

                <li class="@SetClass("Summary")">
                    <a href="~/RobiAdmin/MiscGraphs/Index?user=@ViewBag.UserName&moid=@ViewBag.MoId" title="Summary.">Summary</a>
                </li>

                @*<li>
                    <a href="~/RobiAdmin/OutletList/SrWiseRetailersFromSrLog?user=@ViewBag.UserName&srid=0" title="">SR Wise Retailers From SR Log</a>
                </li>
                <li>
                    <a href="~/RobiAdmin/OutletList/SrWiseRetailersFromRetailer?user=@ViewBag.UserName&srid=0&date=@DateTime.Today.ToString("yyyy-MM-dd")" title="View SR and/or date specific activities from retailer table.">SR Wise Retailers from Retailers</a>
                </li>
                <li>
                    <a href="~/RobiAdmin/OutletList/SrToRetailersCount?user=@ViewBag.UserName&srid=0&date=@DateTime.Today.ToString("yyyy-MM-dd")" title="Displays All SR with work count from retailer table.">SR-Retailers Count from retailer table.</a>
                </li>
                <li>
                    <a target="_blank" href="~/RobiAdmin/DuplicatePhotos/Index?srid=0&date=yyyy-mm-dd" title="Displays duplicate photos list.">Duplicate Photos</a>
                </li>
                <li>
                    <a target="_blank" href="~/RobiAdmin/Reports/RetailersFromAllScenario" title="Retailers From All Scenario.">Evaluating RSP status from all scenario</a>
                </li>
                <li>
                    <a href="~/RobiAdmin/Reports/SignedRspRetailerStats" title="RSP Wise New, Updated, Reevaluated POS Count">RSP-POS Status</a>
                </li>*@

            </ul><!-- /standard nav -->

            <div class="separator-doubled"></div>

            <!-- Stats progress -->
            <!-- /stats progress -->

        </div>
        <!-- /left sidebar -->
        <!-- Main content -->
        <div class="content">
            <div class="outer">
                <div class="inner">
                    <div class="page-header">
                        <!-- Page header -->
                        <h5>@ViewBag.Title</h5>

                    </div><!-- /page header -->

                    <div class="body">
                        <!-- Content container -->
                        <div class="container">
                            @RenderBody()
                        </div>
                        <!-- /content container -->
                    </div>
                </div>
            </div>
        </div>
        <!-- /main content -->

        <!-- Right sidebar -->
        <div class="sidebar" id="right-sidebar">
            <div class="appendable">
                <ul class="user-list block">
                    <li class="active">
                        <a href="#" title="">
                            <span class="contact-name">
                                <strong>Quick Search</strong>
                            </span>
                        </a>
                    </li>

                    <li class="active">
                        <form method="post" target="_blank" action="/RSP/OutletDetails/SearchRetailerById?user=@ViewBag.UserName&moid=@ViewBag.MoId" onsubmit="return ValidateRetailerIdToSearch()">
                            <input type="text" id="SearchRetailerTextBox" name="retailerid" value="" style="margin-bottom:20px; margin-top:20px;" title="type a Retailer ID here and click the button to search" placeholder="type Retailer ID" />
                            <input type="submit" style="height:30px; margin-bottom:20px;" value="Search" />
                        </form>
                    </li>

                    <li class="active">
                        <form method="post" action="~/RobiAdmin/SurveyorManagement/SearchSrById?user=@ViewBag.UserName&moid=@ViewBag.MoId" onsubmit="return ValidateSrIdToSearch()">
                            <input type="text" id="SearchSrTextBox" name="surveyorid" value="" style="margin-bottom:20px; margin-top:20px;" title="type a Surveyor ID here and click the button to search" placeholder="type Surveyor ID" />
                            <input type="submit" style="height:30px; margin-bottom:20px;" value="Search" />
                        </form>
                    </li>


                </ul>
                <!-- /contact list -->

                <div class="separator-doubled"></div>
            </div>
        </div>
        <!-- /right sidebar -->

    </div>

    <!-- /main wrapper -->

    <!--Quick Search -->
    <script>
        function ValidateRetailerIdToSearch() {

            var retailerIdTextBox = document.getElementById('SearchRetailerTextBox');
            var retailerId = retailerIdTextBox.value.trim();
            if (retailerId.length == 0) {
                alert('Please type Retailer ID.'); return false;
            }
            if (isNaN(retailerId)) {
                alert('Retailer ID must be numeric.'); return false;
            }

            return true;
        }

        function ValidateSrIdToSearch() {

            var retailerIdTextBox = document.getElementById('SearchSrTextBox');
            var retailerId = retailerIdTextBox.value.trim();
            if (retailerId.length == 0) {
                alert('Please type Surveyor ID.'); return false;
            }
            if (isNaN(retailerId)) {
                alert('Surveyor ID must be numeric.'); return false;
            }

            return true;
        }
    </script>
</body>
</html>

@functions{
   public string SetClass(string value)
    {
        if (ViewBag.ActiveMenuName==value)
        {
            return "active";
        }
        else
        {
            return "";
        }
    }
}


