@functions{
    public string CalculateNthDay()
    {
        string msg = string.Empty;
        string dayCount = ViewBag.SurveyorsQuantityPerDate.Rows.Count.ToString();
        string lastDigit = dayCount.Substring(dayCount.Length - 1, 1);
        switch (lastDigit)
        {
            case "1":
                msg = dayCount + "st";
                break;
            case "2":
                msg = dayCount + "nd";
                break;
            case "3":
                msg = dayCount + "rd";
                break;
            default:
                msg = dayCount + "th";
                break;
        }
        return msg;
    }
}

